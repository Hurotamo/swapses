{
  "name": "swapses-contracts",
  "version": "1.0.0",
  "description": "Advanced privacy mixer with zero-knowledge proofs",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:zk": "node scripts/test-proofs.js",
    "setup:circuits": "bash scripts/setup-circuits.sh",
    "deploy:base-sepolia": "hardhat run scripts/deploy-zk-mixer.ts --network base-sepolia",
    "deploy:base-mainnet": "hardhat run scripts/deploy-zk-mixer.ts --network base-mainnet",
    "deploy:lisk-sepolia": "hardhat run scripts/deploy-zk-mixer.ts --network lisk-sepolia",
    "deploy:lisk-mainnet": "hardhat run scripts/deploy-zk-mixer.ts --network lisk-mainnet",
    "deploy:local": "hardhat run scripts/deploy-zk-mixer.ts --network localhost",
    "verify:base-sepolia": "hardhat verify --network base-sepolia",
    "verify:base-mainnet": "hardhat verify --network base-mainnet",
    "verify:lisk-sepolia": "hardhat verify --network lisk-sepolia",
    "verify:lisk-mainnet": "hardhat verify --network lisk-mainnet",
    "circuit:compile": "circom circuits/mixer.circom --r1cs --wasm --sym --c",
    "circuit:setup": "snarkjs groth16 setup mixer.r1cs ptau/powersOfTau28_hez_final_16.ptau mixer_0000.zkey",
    "circuit:contribute": "snarkjs zkey contribute mixer_0000.zkey mixer_final.zkey",
    "circuit:export": "snarkjs zkey export verificationkey mixer_final.zkey verification_key.json",
    "proof:generate": "node scripts/generate-proof.js",
    "proof:verify": "node scripts/verify-proof.js"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.17.1",
    "hardhat-deploy": "^0.12.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-etherscan": "^3.1.7",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-spdx-license-identifier": "^3.0.1",
    "hardhat-tracer": "^1.0.0-beta.19",
    "hardhat-typechain": "^3.1.2",
    "hardhat-watcher": "^2.2.1",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "web3": "^1.10.0",
    "circom": "^2.1.4",
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.0",
    "circomlibjs": "^0.1.7",
    "ffjavascript": "^0.2.29",
    "wasmsnark": "^0.7.0",
    "websnark": "^0.1.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^3.0.2",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@typechain/ethers-v5": "^11.1.1",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.0",
    "chai": "^4.3.7",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.17.1",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6",
    "dotenv": "^16.0.0"
  },
  "keywords": [
    "privacy",
    "mixer",
    "zero-knowledge",
    "zk-snarks",
    "ethereum",
    "blockchain",
    "cryptography"
  ],
  "author": "Swapses Team",
  "license": "MIT"
}
